{
    "name": "zikula/routes-module",
    "version": "3.1.0-dev",
    "description": "Routes management",
    "type": "zikula-system-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Zikula contributors",
            "email": "info@ziku.la",
            "homepage": "https://ziku.la",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\RoutesModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/contracts": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/form": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/security": "^5.2",
        "symfony/string": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/workflow": "^5.2",

        "zikula/core-bundle": "self.version",
        "zikula/formextension-bundle": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/groups-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/pagelock-module": "1.*",
        "zikula/permissions-module": "self.version",
        "zikula/settings-module": "self.version",
        "zikula/theme-module": "self.version",
        "zikula/users-module": "self.version",
        "zikula/sortable-columns": "1.*",

        "jms/i18n-routing-bundle": "3.*",
        "friendsofsymfony/jsrouting-bundle": "2.*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=3.0.0 <4.0.0",
            "class": "Zikula\\RoutesModule\\ZikulaRoutesModule",
            "displayname": "Routes",
            "url": "routes",
            "icon": "fas fa-map-marked-alt",
            "capabilities": {
                "admin": {
                    "route": "zikularoutesmodule_route_adminindex"
                },
                "user": {
                    "route": "zikularoutesmodule_route_index"
                }
            },
            "securityschema": {
                "ZikulaRoutesModule::": "::",
                "ZikulaRoutesModule:Route:": "Route ID::",
                "ZikulaRoutesModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
